    "test": "npm run test-no-lint",
    "test-no-lint": "BABEL_ENV=test babel-node spec/run.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jasmine": "^3.2.0"
  },
  "dependencies": {
    "@openactive/data-model-validator": "github:openactive/data-model-validator",
    "jsonpath": "^1.0.0",
    "node-fetch": "^2.2.0"
