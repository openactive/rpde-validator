{
  "name": "@openactive/rpde-validator",
  "version": "0.1.0",
  "description": "A library to walk and validate an RPDE feed",
  "homepage": "http://openactive.io",
  "author": "Pete Walker <pete@imin.co> (https://github.com/petewalker)",
  "keywords": [
    "openactive",
    "odi",
    "validator",
    "json"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": "8.11.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openactive/rpde-validator.git"
  },
  "bugs": {
    "url": "https://github.com/openactive/rpde-validator/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "postinstall": "npm run build",
    "lint": "eslint \"src/**/*.js\"",
